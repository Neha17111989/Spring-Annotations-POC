<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans 
    http://www.springframework.org/schema/beans/spring-beans.xsd 
    http://www.springframework.org/schema/context 
    http://www.springframework.org/schema/context/spring-context.xsd">
	<bean id="address" class="com.altimetrik.Address">
		<property name="zipcode" value="140501"></property>
		<property name="country" value="India"></property>
	</bean>
	<bean id="user" class="com.altimetrik.User.java">
		<property name="name" value="Neha Mehta"></property>
		<property name="companyName" value="Altimetrik"></property>
		<property name="address" value="address"></property>




	</bean>

</beans>